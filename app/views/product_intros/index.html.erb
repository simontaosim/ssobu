<p id="notice"><%= notice %></p>

<h1>Listing Product Intros</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_intros.each do |product_intro| %>
      <tr>
        <td><%= link_to 'Show', product_intro %></td>
        <td><%= link_to 'Edit', edit_product_intro_path(product_intro) %></td>
        <td><%= link_to 'Destroy', product_intro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product intro', new_product_intro_path %>

<p id="notice"><%= notice %></p>

<h1>Listing Withdraw Records</h1>

<table>
  <thead>
    <tr>
      <th>Bank name</th>
      <th>Bank card</th>
      <th>Amount</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @withdraw_records.each do |withdraw_record| %>
      <tr>
        <td><%= withdraw_record.bank_name %></td>
        <td><%= withdraw_record.bank_card %></td>
        <td><%= withdraw_record.amount %></td>
        <td><%= link_to 'Show', withdraw_record %></td>
        <td><%= link_to 'Edit', edit_withdraw_record_path(withdraw_record) %></td>
        <td><%= link_to 'Destroy', withdraw_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Withdraw record', new_withdraw_record_path %>
